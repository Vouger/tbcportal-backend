{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6",
         "es2018",
         "esnext.asynciterable",
         "dom"
      ],
      "target": "es2018",
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "strictPropertyInitialization": false,
      "sourceMap": true
   }
}